<form method="POST", action="/add">
<%= token_tag %>
  <div class="form-group p-4">
    <label for="exampleFormControlTextarea1">Enter City</label>
    <input name="location" value="<%= params["location"]%>" class="form-control border border-dark" id="location" rows="1"/>
  </div>
  <div class="form-group p-4">
    <label for="exampleFormControlTextarea1">Enter Country</label>
    <input name="country" value="<%= params["country"]%>" class="form-control border border-dark" id="country" rows="1"/>
  </div>
  <button type="submit" class="btn btn-primary m-4">Submit</button>
</form>

<% if @output.nil?%>
    Make a query
    <% else %>
<%= render 'home/card'%>
<% end %>
<%= @output%>
